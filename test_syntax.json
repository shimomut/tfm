{
  "name": "TFM Text Viewer Test",
  "version": "1.0.0",
  "description": "Test file for JSON syntax highlighting",
  "features": [
    "syntax highlighting",
    "line numbers",
    "scrolling",
    "multiple file formats"
  ],
  "supported_formats": {
    "programming": ["python", "javascript", "java", "c", "cpp"],
    "markup": ["html", "xml", "markdown"],
    "data": ["json", "yaml", "csv"],
    "config": ["ini", "toml", "conf"]
  },
  "settings": {
    "show_line_numbers": true,
    "wrap_lines": false,
    "syntax_highlighting": true,
    "color_scheme": "default"
  },
  "test_data": {
    "numbers": [1, 2, 3, 4, 5],
    "boolean": true,
    "null_value": null,
    "nested": {
      "level1": {
        "level2": "deep nesting test"
      }
    }
  }
}